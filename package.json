{
  "name": "allyourrecipes",
  "version": "0.0.1",
  "description": "Nuxt.js project",
  "author": "cavejay <michael@sablecliff.com>",
  "private": true,
  "scripts": {
    "dev-pwsh": "pwsh ./runProj.ps1",
    "dev": "nodemon -w ./server/** ./server/index.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "docker": "docker build . --tag talking-puppet",
    "testdocker": "docker run -p 8080:80 --name tp --rm talking-puppet:latest",
    "pushdocker": "docker push talkingpuppet.azurecr.io/talking-puppet",
    "cleandocker": "docker kill tp"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "ajv": "^6.5.3",
    "axios-cache-plugin": "^0.1.0",
    "buefy": "^0.7.3",
    "common-tags": "^1.8.0",
    "cottage": "^3.0.1",
    "debounce-async": "0.0.2",
    "graphql": "^14.1.1",
    "koa": "^2.4.1",
    "koa-better-body": "^3.0.4",
    "koa-bodyparser": "^4.2.0",
    "koa-graphql": "^0.8.0",
    "koa-mount": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "markdown-it": "^8.4.2",
    "mongoose": "^5.4.19",
    "nedb-promise": "^2.0.1",
    "nuxt": "^2.4.5",
    "nuxt-buefy": "^0.3.4",
    "nuxt-env": "^0.1.0",
    "pino": "^5.11.1",
    "source-map-support": "^0.4.15",
    "tiptap": "^1.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "cross-env": "^5.0.1",
    "eslint": "^5.15.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.16.4",
    "scmp": "^2.0.0"
  }
}
